{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/app.py"
      }
    ],
    "builds": [
      {
        "src": "/api/app.py /api/helpers.py",
        "use": "@vercel/python"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "FLASK_APP": "/api/app.py"
    }
  }